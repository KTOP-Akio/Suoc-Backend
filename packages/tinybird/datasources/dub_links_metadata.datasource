SCHEMA >
    `timestamp` DateTime `json:$.timestamp`,
    `link_id` String `json:$.link_id`,
    `domain` String `json:$.domain`,
    `key` String `json:$.key`,
    `url` String `json:$.url`,
    `team_id` String `json:$.team_id`,
    `deleted` UInt8 `json:$.deleted`

ENGINE "MergeTree"
ENGINE_SORTING_KEY "timestamp, link_id, team_id, deleted"