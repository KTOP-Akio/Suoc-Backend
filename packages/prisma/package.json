  },
  "devDependencies": {
    "@types/node": "18.11.9",
    "prisma": "5.18.0",
    "tsx": "^4.11.0",
    "typescript": "5.2.2",
    "dotenv-flow": "^4.0.0",
    "dotenv-flow-cli": "^1.1.1"
  },
  "exports": {
    ".": "./index.ts",
    "./edge": "./edge.ts"
  }
}
