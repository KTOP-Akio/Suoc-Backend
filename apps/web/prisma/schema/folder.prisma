model Folder {
  id        String   @id @default(cuid())
  name      String
  projectId String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  project Project @relation(fields: [projectId], references: [id], onDelete: Cascade)
  links   Link[]

  @@unique([name, projectId])
  @@index(projectId)
}
